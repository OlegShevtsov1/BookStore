%main.container.general-main-wrap
  .text-center
    %h3.general-login-title=t('devise.log_in')
    = render 'devise/shared/facebook'
    %p.general-login-text or
  = simple_form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: 'general-form'}) do |f|
    = render 'devise/registrations/new_form', f: f, minimum_password_length: @minimum_password_length
